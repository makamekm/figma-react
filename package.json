{
  "name": "figma-react",
  "description": "This is a tool to help you export Figma project into React mockups.",
  "version": "1.0.3",
  "private": false,
  "author": "Maxim Karpov <makame@mail.ru>",
  "repository": {
    "type": "git",
    "url": "https://github.com/makamekm/figma-react.git"
  },
  "keywords": [
    "figma",
    "react",
    "exporter",
    "tool",
    "cli"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "release:patch": "release-it patch --git.tagName='v${version}' --github.release",
    "release:minor": "release-it minor --git.tagName='v${version}' --github.release",
    "release:major": "release-it patch --git.tagName='v${version}' --github.release"
  },
  "bin": {
    "figma-react": "./cli.js"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "release-it": "^12.6.0"
  }
}
